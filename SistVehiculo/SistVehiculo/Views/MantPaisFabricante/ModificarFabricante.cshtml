@model SistVehiculo.Models.pa_RetornaPaisFabricanteID_Result

@section css{
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
    <link href="~/Content/mantMarcaVehiculos/mantMarcaVehiculos.css" rel="stylesheet" />
}

@{
    ViewBag.Title = "Modificar Fabricante";
    Layout = "~/Views/Shared/_LayoutPersonalizado.cshtml";
}

<h2>Modificar Fabricante</h2>

<form id="frmModificaMarcaVehiculo" method="post">
    <div class="form-group">
        <label>Código:</label>
        <input type="text" name="codigo" id="codigo" class="form-control" value="@Model.codigo" />
    </div>

    <div class="form-group">
        <label>País Fabricante:</label>
        <input type="text" name="pais" id="pais" class="form-control" value="@Model.pais" />
    </div>
    <div class="form-group">
        <input type="submit" value="Modificar" id="btnAceptar" class="btn btn-success btnModificarMarca" />
    </div>
</form>
<a href="~/MantPaisFabricante/ListaFabricante" class="btn btn-primary">Regresar a la lista</a>

@section scripts{
    <script src="https://code.jquery.com/ui/1.12.0/jquery-ui.min.js"></script>
    <script src="~/Scripts/MantFabricante/mantFabricante.js"></script>
}