@model SistVehiculo.Models.pa_RetornaTiposVehiculoID_Result

@section css{
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
}

@{
    ViewBag.Title = "Elimina Tipos Vehículo";
    Layout = "~/Views/Shared/_LayoutPersonalizado.cshtml";
}

<h2>Eliminar Tipos Vehículo</h2>


<form id="frmModificaTiposVehiculo" method="post">
    <div class="form-group">
        <label>Código:</label>
        <input type="text" name="codigo" id="codigo" class="form-control" value="@Model.codigo" readonly/>
    </div>

    <div class="form-group">
        <label>Tipo:</label>
        <input type="text" name="tipo" id="tipo" class="form-control" value="@Model.tipo" readonly/>
    </div>
    <div class="form-group">
        <input type="submit" value="Eliminar" id="btnAceptar" />
    </div>
    <a href="~/MantTipoVehiculos/ListaTipoVehiculos">Regresar a la lista</a>
</form>

@section scripts{
    <script src="https://code.jquery.com/ui/1.12.0/jquery-ui.min.js"></script>
    <script src="~/Scripts/mantTiposVehiculos/mantTiposVehiculos.js"></script>
}