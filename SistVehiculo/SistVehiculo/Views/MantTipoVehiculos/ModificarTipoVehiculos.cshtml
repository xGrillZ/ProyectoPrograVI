@model SistVehiculo.Models.pa_RetornaTiposVehiculoID_Result

@section css{
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
    <link href="~/Content/mantTipoVehiculos/mantTipoVehiculos.css" rel="stylesheet" />
}

@{
    ViewBag.Title = "Modifica Tipos Vehículo";
    Layout = "~/Views/Shared/_LayoutPersonalizado.cshtml";
}

<h2>Modificar Tipos Vehículo</h2>


<form id="frmModificaTiposVehiculo" method="post">
    <div class="form-group">
        <label>Código:</label>
        <input type="text" name="codigo" id="codigo" class="form-control" value="@Model.codigo" />
    </div>

    <div class="form-group">
        <label>Tipo:</label>
        <input type="text" name="tipo" id="tipo" class="form-control" value="@Model.tipo" />
    </div>
    <div class="form-group">
        <input type="submit" value="Modificar" id="btnAceptar" class="btn btn-success btnModificarTipoVehiculo" />
    </div>
</form>
<a href="~/MantTipoVehiculos/ListaTipoVehiculos" class="btn btn-primary">Regresar a la lista</a>

@section scripts{
    <script src="https://code.jquery.com/ui/1.12.0/jquery-ui.min.js"></script>
    <script src="~/Scripts/mantTiposVehiculos/mantTiposVehiculos.js"></script>
}