
@model SistVehiculo.Models.pa_RetornaVehiculosID_Result

@section css{
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
    <link href="~/Content/mantVehiculos/mantVehiculos.css" rel="stylesheet" />
}

@{
    ViewBag.Title = "Elimina Vehículo";
    Layout = "~/Views/Shared/_LayoutPersonalizado.cshtml";
}

<h2>Eliminar Vehículo</h2>


<form id="frmEliminaVehiculo" method="post">
    <div class="row col-sm-6">
        <div class="form-group col-6">
            <label>Id de Vehiculo:</label>
            <input type="text" name="idVehiculos" id="idVehiculos" class="form-control" value="@Model.idVehiculos" readonly />
        </div>
        <div class="form-group col-6">
            <label>Placa:</label>
            <input type="text" name="placa" id="placa" class="form-control" value="@Model.placa" readonly />
        </div>
        <div class="form-group col-6">
            <label>Tipo de Vehiculo:</label>
            <input type="text" name="tipoVehiculo" id="tipoVehiculo" class="form-control" value="@Model.tipoVehiculo" readonly />
        </div>
        <div class="form-group col-6">
            <label>Marca Vehiculo:</label>
            <input type="text" name="marcaVehiculo" id="marcaVehiculo" class="form-control" value="@Model.marcaVehiculo" readonly />
        </div>
        <div class="form-group col-6">
            <label>Número de Puertas:</label>
            <input type="text" name="numeroPuerta" id="numeroPuerta" class="form-control" value="@Model.numeroPuerta" readonly />
        </div>
        <div class="form-group col-6">
            <label>Número de Ruedas:</label>
            <input type="text" name="numeroRueda" id="numeroRueda" class="form-control" value="@Model.numeroRueda" readonly />
        </div>
    </div>

    <div class="form-group">
        <input type="submit" value="Eliminar" id="btnAceptar" class="form-control btn btn-success btnEliminarVehiculo" />
    </div>
</form>
<a href="~/MantVehiculos/ListaVehiculos" class="btn btn-primary">Regresar a la lista</a>

@section scripts{
    <script src="https://code.jquery.com/ui/1.12.0/jquery-ui.min.js"></script>
    <script src="~/Scripts/mantVehiculos/mantVehiculos.js"></script>
}